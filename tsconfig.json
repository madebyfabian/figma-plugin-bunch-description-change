{
	"compilerOptions": {
		"target": "es2017",
		"moduleResolution": "node",
		"module": "esnext",
		"typeRoots": ["./node_modules/@types", "./node_modules/@figma"],
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"]
		},
		"jsx": "preserve",
		"allowJs": true
	},
	"vueCompilerOptions": {
		"target": 2
	},
	"include": ["src/**/*.ts", "src/**/*.vue"]
}
